{
  "name": "tradeline247",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "server.mjs",
  "engines": {
    "node": ">=20.19.0 <21",
    "npm": ">=8"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "node server.mjs",
    "preview": "vite preview --host 0.0.0.0 --port 5000",
    "prebuild": "node -e \"if(process.env.SKIP_VERIFY_CANON==='1'){process.exit(0)}\" && node scripts/verify-canon.cjs || node scripts/verify-canon.js",
    "scan": "npm audit --omit=dev || true",
    "harden": "node scripts/verify-runtime.cjs",
    "dev:ui": "vite",
    "dev:api": "nodemon -q --signal SIGTERM server.mjs",
    "dev:all": "concurrently -k -n UI,API -c green,blue \"npm:dev:ui\" \"npm:dev:api\"",
    "preview:prod": "vite build && node server.mjs",
    "seo": "node scripts/seo-patch.mjs",
    "landing:dev": "npm --prefix landing run dev",
    "landing:build": "npm --prefix landing run build",
    "landing:preview": "npm --prefix landing run preview",
    "build:client": "vite build",
    "postbuild": "node scripts/stamp-version.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "openai": "^4.56.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "resend": "^3.2.0",
    "tsscmp": "^1.0.6",
    "twilio": "^4.23.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "react-router-dom": "^7.9.1",
    "vite": "^5.4.20"
  }
}
