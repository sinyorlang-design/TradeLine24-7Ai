# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
# Twilio helper reads TWILIO_AUTH_TOKEN to validate signatures
TWILIO_AUTH_TOKEN=__set_in_runtime__
PUBLIC_BASE_URL=https://your-domain.com
# Public host used when validating Twilio signatures behind a proxy
PUBLIC_HOSTNAME=www.tradeline247ai.com

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Resend Configuration
RESEND_API_KEY=your_resend_api_key
FROM_EMAIL=voicemail@yourdomain.com
NOTIFICATION_EMAIL=admin@yourdomain.com

# Server Configuration
PORT=5000
HOST=0.0.0.0
